<div class="container">
<h1>Bienvenido</h1>
<p> Estas son tus tareas </p>
  <div class="row">
   <% @todos.each_slice(4) do |row| %>
    <% row.each do |todo| %>
      <div class="col">
       <%= image_tag todo.photo, class: 'img-responsive' %>
       <h3> <%= todo.name%> </h3>
       <P> <%= todo.description%> </p>
      </div>
     <% end %>
     <div class="w-100"></div>
   <% end %>
  </div>
</div>
